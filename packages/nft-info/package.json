{
  "name": "@smitskecbs/nft-info",
  "version": "0.1.0",
  "description": "Solana NFT / Digital Asset inspector using Helius DAS getAsset (v1) + Kit-first version (v2).",
  "main": "dist/index.mjs",
  "type": "module",
  "scripts": {
    "dev": "tsx src/index.ts",
    "dev:kit": "tsx src/index.kit.ts",
    "build": "tsup src/index.ts src/index.kit.ts --format esm --dts --out-dir dist"
  },
  "dependencies": {
    "@solana/web3.js": "^1.95.2",
    "@solana/kit": "^2.0.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "tsup": "^8.5.1"
  }
}
